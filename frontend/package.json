{
  "name": "odh-tec-frontend",
  "version": "2.1.0",
  "description": "Frontend for the Open Data Hub Tools & Extensions Companion.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rh-aiservices.bu/odh-tec.git"
  },
  "homepage": "",
  "bugs": {
    "url": "https://github.com/rh-aiservices.bu/odh-tec/issues"
  },
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "prebuild": "npm run type-check && npm run clean",
    "dr:surge": "node dr-surge.js",
    "build": "webpack --config ./webpack.prod.js && npm run dr:surge",
    "start": "sirv dist --cors --single --host --port 8888",
    "start:dev": "webpack serve --color --progress --config ./webpack.dev.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "eslint": "eslint --ext .tsx,.js ./src/",
    "lint": "npm run eslint",
    "format": "prettier --check --write ./src/**/*.{tsx,ts}",
    "type-check": "tsc --noEmit",
    "ci-checks": "npm run type-check && npm run lint && npm run test:coverage",
    "build:bundle-profile": "webpack --config ./webpack.prod.js --profile --json > stats.json",
    "bundle-profile:analyze": "npm run build:bundle-profile && webpack-bundle-analyzer ./stats.json",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@redhat-cloud-services/eslint-config-redhat-cloud-services": "^2.0.12",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^10.0.0",
    "@types/victory": "^35.0.0",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "axios-mock-adapter": "^1.22.0",
    "babel-plugin-transform-imports": "^2.0.0",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "dotenv-webpack": "^8.1.0",
    "eslint": "^8.9.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.3",
    "i18next": "^23.16.8",
    "i18next-http-backend": "^2.7.3",
    "imagemin": "^9.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.2",
    "monaco-editor-webpack-plugin": "^7.1.1",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prop-types": "^15.8.1",
    "raw-loader": "^4.0.2",
    "react": "^18.3.1",
    "react-axe": "^3.5.4",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^18.3.1",
    "react-i18next": "^14.1.3",
    "react-router-dom": "^7.1.5",
    "regenerator-runtime": "^0.14.1",
    "rimraf": "^6.0.1",
    "sirv-cli": "^3.0.1",
    "style-loader": "^4.0.0",
    "svg-url-loader": "^8.0.0",
    "terser-webpack-plugin": "^5.3.14",
    "ts-jest": "^29.3.1",
    "ts-loader": "^9.5.2",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "tslib": "^2.8.1",
    "typescript": "^5.8.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.98.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
